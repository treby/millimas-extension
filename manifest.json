{
    "manifest_version": 2,
    "name": "Millimas Extension",
    "version": "0.0.1",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "tabs",
        "http://imas.gree-apps.net/*",
        "http://*.gree.net/*",
        "http://*.gree.jp/*"
    ],
    "browser_action": {
        "default_title": "Millimas Extension"
    },
    "background": {
        "persistent": true,
        "page": "background.html"
    }
}
